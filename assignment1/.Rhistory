Matrix_multip <- function(M1,M2){
M=matrix(0,nrow = nrow(M1) , ncol = ncol(M2))
for(j in 1:nrow(M1)){
for (s in 1:nrow(M1)) {
for (t in 1:ncol(M1)) {
M[j,s] <- M[j,s] + M1[j,t]*M2[t,s]
}
}
}
print(M)
}
